{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*\\.js)", "dest": "/$1" },
    { "src": "/(.*\\.css)", "dest": "/$1" },
    { "src": "/(.*\\.png)", "dest": "/$1" },
    { "src": "/(.*\\.svg)", "dest": "/$1" },
    { "src": "/(.*\\.ico)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
